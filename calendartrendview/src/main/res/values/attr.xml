<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SimplePaperView">
        <attr name="android:padding" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingStart" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingEnd" />
        <attr name="android:paddingRight" />
        <attr name="android:paddingBottom" />
    </declare-styleable>

    <declare-styleable name="CalendarTrendView">
        <attr name="ctv_startFrom" format="enum">
            <enum name="nowhere" value="0"/>
            <enum name="origin" value="1"/>
            <enum name="firstValue" value="2"/>
        </attr>
        <attr name="ctv_stepLineColor" format="color" />
        <attr name="ctv_dayLabelColor" format="color" />
        <attr name="ctv_monthLabelColor" format="color" />
        <attr name="ctv_todayLabelColor" format="color" />
        <attr name="ctv_xUnitMeasure" format="dimension" />
        <attr name="ctv_yUnitMeasure" format="dimension" />
        <attr name="ctv_showToday" format="boolean" />
    </declare-styleable>

</resources>
